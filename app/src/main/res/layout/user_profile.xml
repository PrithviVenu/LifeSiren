<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@mipmap/lifesiren"
        android:layout_centerHorizontal="true" />
    <TextView
        android:id="@+id/id"
        android:layout_below="@id/imageView"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="id"
        android:textColor="@android:color/widget_edittext_dark"
        />
    <TextView
        android:id="@+id/id1"
        android:layout_below="@id/id"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@android:color/holo_blue_dark"
        />
    <TextView
        android:id="@+id/uName"
        android:layout_below="@id/id1"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:textColor="@android:color/widget_edittext_dark"
        />
    <TextView
        android:id="@+id/uName1"
        android:layout_below="@id/uName"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@android:color/holo_blue_dark"
        />


    <TextView
        android:id="@+id/uCity"
        android:layout_below="@id/uName1"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City"
        android:textColor="@android:color/widget_edittext_dark"
        />
    <TextView
        android:id="@+id/uCity1"
        android:layout_below="@id/uCity"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@android:color/holo_blue_dark"
        />
    <TextView
        android:id="@+id/uAddress"
        android:layout_below="@id/uCity1"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address"
        android:textColor="@android:color/widget_edittext_dark"
        />
    <TextView
        android:id="@+id/uAddress1"
        android:layout_below="@id/uAddress"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@android:color/holo_blue_dark"
        />
    <TextView
        android:id="@+id/uContact_Number"
        android:layout_below="@id/uAddress1"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact_Number"
        android:textColor="@android:color/widget_edittext_dark"
        />
    <TextView
        android:id="@+id/uContact_Number1"
        android:layout_below="@id/uContact_Number"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@android:color/holo_blue_dark"
        />

</RelativeLayout>
